{
    "name": "affinity",
    "type": "dark",
    "colors": {
        "activityBar.background": "#F09483",
		"activityBarBadge.background": "#E95678",
		"badge.background": "#ffffff1a",
		"editor.background": "#242424",
		"editor.foreground": "#cccccc",
		"editor.lineHighlightBackground": "#ffffff0b",
		// "editor.selectionBackground": "#204182cc",
		"editorGutter.background": "#ffffff0b",
		"editorSuggestWidget.background": "#303030",
		// "editorSuggestWidget.highlightForeground": "#cc6e2f",
		// "editorSuggestWidget.selectedBackground": "#204182cc",
        "sideBar.background": "#222222",
		"sideBar.border": "#ffffff1a",
		"statusBar.background": "#ffffff1a",
		"statusBar.foreground": "#cccccc",
		"tab.activeBackground": "#ffffff1a",
		"tab.activeBorder": "#F09483",
		"tab.inactiveBackground": "#ffffff0b",
		"titleBar.activeBackground": "#ffffff1a",
		"titleBar.activeForeground": "#cccccc",
		"titleBar.inactiveBackground": "#ffffff10",
    },
    "tokenColors": [
        {
            "name": "keywords-color",
            "scope": [
                "keyword",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.delete",
                "markup.bold",
                "storage",
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#B877DB"
            }
        },
        {
            "name": "outer-color",
            "scope": [
                "entity.name.variable",
                "entity.name.tag",
                "punctuation.definition.tag",
                "variable",
                "markup.heading",
                "entity.name.section",
                "support.variable",
                "support.type.property-name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#E95678"
            }
        },
        {
            "name": "middle-color",
            "scope": [
                "constant",
                "entity.other.attribute-name",
                "keyword.other.unit",
                "markup.inline.raw",
                "markup.fenced_code.block",
                "string.regexp",
                "string.other.link",
                "punctuation.definition.list"
            ],
            "settings": {
                "foreground": "#F09483"
            }
        },
        {
            "name": "inner-color",
            "scope": [
                "entity.other.inherited-class",
                "entity.other.attribute-name.pseudo-element",
                "entity.other.attribute-name.pseudo-class",
                "markup.quote",
                "markup.underline.link",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#FAB795"
            }
        },
        {
            "name": "core-color",
            "scope": ["entity.name", "entity.name.type", "storage.type.cs", "support", "variable.language"],
            "settings": {
                "foreground": "#FAC29A"
            }
        },
        {
            "name": "function-color",
            "scope": [
                "constant.character.escape",
                "entity.name.function",
                "entity.other.attribute-name.id",
                "markup.italic",
                "support.function"
            ],
            "settings": {
                "foreground": "#fffb00"
            }
        },
        {
            "name": "text-color",
            "scope": [
                "comment",
                "keyword.operator",
                "support.type.property-name.css",
                "string.template meta.embedded",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#BBBBBB"
            }
        },
        {
            "name": "Bold",
            "scope": ["constant.language"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Italic",
            "scope": ["comment"],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Underline",
            "scope": "",
            "settings": {
                "fontStyle": "underline"
            }
        }
    ]
}
