{
    "name": "Affinity",
    "type": "dark",
    "colors": {
        "activityBar.background": "#202020",
        "activityBarBadge.background": "#F09483",
        "badge.background": "#ffffff1a",
        "editor.background": "#242424",
        "editor.foreground": "#cccccc",
        "editor.lineHighlightBackground": "#ffffff0b",
        "editorSuggestWidget.background": "#303030",
        "sideBar.background": "#202020",
        "sideBarSectionHeader.background": "#202020",
        "sideBarSectionHeader.border": "#5a5a5a",
        "statusBar.background": "#202020",
        "statusBar.foreground": "#cccccc",
        "editorGroupHeader.tabsBackground": "#242424",
        "tab.activeBackground": "#242424",
        "tab.activeBorder": "#F09483",
        "tab.inactiveBackground": "#242424",
        "titleBar.activeBackground": "#202020",
        "titleBar.activeForeground": "#cccccc",
        "titleBar.inactiveBackground": "#242424"
    },
    "tokenColors": [
        {
            "name": "keywords-color",
            "scope": [
                "keyword",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.delete",
                "markup.bold",
                "storage",
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#a3586d"
            }
        },
        {
            "name": "outer-color",
            "scope": [
                "entity.name.variable",
                "entity.name.tag",
                "support.class.component",
                "punctuation.definition.tag",
                "variable",
                "markup.heading",
                "entity.name.section",
                "support.variable",
                "support.type.property-name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#f46a4e"
            }
        },
        {
            "name": "middle-color",
            "scope": [
                "constant",
                "entity.other.attribute-name",
                "keyword.other.unit",
                "markup.inline.raw",
                "markup.fenced_code.block",
                "string.regexp",
                "string.other.link",
                "punctuation.definition.list"
            ],
            "settings": {
                "foreground": "#f4874c"
            }
        },
        {
            "name": "inner-color",
            "scope": [
                "entity.other.inherited-class",
                "entity.other.attribute-name.pseudo-element",
                "entity.other.attribute-name.pseudo-class",
                "markup.quote",
                "markup.underline.link",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#f3af5a"
            }
        },
        {
            "name": "core-color",
            "scope": [
                "entity.name",
                "entity.name",
                "entity.name.type",
                "storage.type.cs",
                "support",
                "variable.language"
            ],
            "settings": {
                "foreground": "#f3af5a"
            }
        },
        {
            "name": "function-color",
            "scope": [
                "constant.character.escape",
                "entity.name.function",
                "entity.other.attribute-name.id",
                "markup.italic",
                "support.function"
            ],
            "settings": {
                "foreground": "#a3586d"
            }
        },
        {
            "name": "text-color",
            "scope": [
                "comment",
                "keyword.operator",
                "support.type.property-name.css",
                "string.template meta.embedded",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#BBBBBB"
            }
        },
        {
            "name": "Bold",
            "scope": [],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Italic",
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Underline",
            "scope": [],
            "settings": {
                "fontStyle": "underline"
            }
        }
    ]
}